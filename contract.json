{
    "contracts": [
        {
            "id": "cocoon.2",
            "repo": {
                "url": "https://github.com/ncodes/cocoon-example-01",
                "version": "",
                "language": "go",
                "link": ""
            },
            "build": {
                "pkgMgr": "glide"
            },
            "resources": {
                "memory": "1g",
                "cpuShare": "1x"
